{
  "collectionName": "components_analytics_google_analytics",
  "info": {
    "displayName": "Google Analytics",
    "description": "Google Analytics 4 configuration"
  },
  "options": {},
  "attributes": {
    "enabled": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "measurementId": {
      "type": "string",
      "required": false,
      "regex": "^G-[A-Z0-9]{10}$"
    },
    "trackingId": {
      "type": "string",
      "required": false,
      "regex": "^UA-[0-9]+-[0-9]+$"
    },
    "enhancedEcommerce": {
      "type": "boolean",
      "default": true
    },
    "customDimensions": {
      "type": "json",
      "required": false
    },
    "customMetrics": {
      "type": "json",
      "required": false
    }
  }
}
