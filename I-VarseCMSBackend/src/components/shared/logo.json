{
  "collectionName": "components_elements_logos",
  "info": {
    "displayName": "logo"
  },
  "options": {},
  "attributes": {
    "logoThemeType": {
      "type": "enumeration",
      "enum": [
        "light",
        "dark"
      ]
    },
    "logoText": {
      "type": "string"
    },
    "logoImageLight": {
      "type": "media",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "logoThemeType"
            },
            "light"
          ]
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "logoImageDark": {
      "type": "media",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "logoThemeType"
            },
            "dark"
          ]
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  },
  "config": {}
}
